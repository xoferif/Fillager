@model Fillager.Models.Menu.MenuItemListModel

<div class="navbar-collapse collapse">
    <ul class="nav navbar-nav">

        @* ReSharper disable Html.TagNotResolved *@
        @foreach (var menuItem in Model.MenuItems.Where(m => m.ParentId == 1).ToList())
        {
            <menulink controller-name="@menuItem.ControllerName" action-name="@menuItem.ActionName" menu-text="@menuItem.MenuItemText" menu-id="@menuItem.Id"></menulink>
        }
        @* ReSharper restore Html.TagNotResolved *@
    </ul>
   
</div>